t Btn from '@/components/Btn/Btn';\r\n  \r\n  export default {\r\n    name: 'HallPaintingsPage',\r\n    components: {\r\n      Table,\r\n      Btn,\r\n    },\r\n    setup() {\r\n      const store = useStore();\r\n      const router = useRouter();\r\n  \r\n      onMounted(() => {\r\n        fetchItems(store);\r\n      });\r\n  \r\n      const hallId = router.currentRoute.value.query.hallId;\r\n      const items = computed(() => {\r\n            const paintings = selectItemsByHallId(store, hallId);\r\n            return paintings.map(painting => {\r\n            const hall = selectItemById(store, painting.id_hall);\r\n            return { ...painting, id_hall: hall ? hall.name : '' };\r\n            });\r\n        });\r\n  \r\n      const onClickRemove = id => {\r\n        const isConfirmRemove = confirm('Вы действительно хотите удалить запись?')\r\n        if (isConfirmRemove) {\r\n          removeItem(store, id)\r\n        }\r\n      };\r\n  \r\n      const onClickEdit = id => {\r\n        router.push({ name: 'PaintingEdit', params: { id } })\r\n      };\r\n      const getImgUrl = imgPath => {\r\n      return require('@/assets/' + imgPath.slice(7));\r\n      };\r\n  \r\n  \r\n      return {\r\n        items,\r\n        onClickRemove,\r\n        onClickEdit,\r\n        getImgUrl\r\n        \r\n      };\r\n    },\r\n  }\r\n  </script>\r\n  \r\n  <style module lang=\"scss\">\r\n  .root {\r\n    .create {\r\n      margin-top: 16px;\r\n    }\r\n  }\r\n  </style>\r\n  "]}]}