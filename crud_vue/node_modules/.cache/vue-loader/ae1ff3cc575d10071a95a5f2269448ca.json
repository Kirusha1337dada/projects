CAAC,CAAC,EAAE;QACL,CAAC,CAAC,CAAC,CAAC,CAAC;QACL,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;QACb,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;QACX,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC,CAAC;;MAEV,CAAC;IACH,CAAC;EACH","file":"C:/Users/karls/Desktop/crud_vue/src/views/HallPaintingsPage.vue","sourceRoot":"","sourcesContent":["<template>\r\n    <div :class=\"$style.root\">\r\n      <Table\r\n        :headers=\"[\r\n          { value: 'id', text: 'ID' },\r\n          { value: 'name', text: 'Имя' },\r\n          { value: 'img', text: 'Изображение' },\r\n          { value: 'year', text: 'Год' },\r\n          { value: 'description', text: 'Описание' },\r\n          { value: 'id_hall', text: 'Зал' },\r\n          { value: 'control', text: 'Действие' },\r\n        ]\"\r\n        :items=\"items\"\r\n      >\r\n        <template v-slot:img=\"{ item }\">\r\n          <img :src=\"getImgUrl(item.img)\" alt=\"Изображение\" style=\"width: 100px; height: auto;\">\r\n        </template>\r\n        <template v-slot:control=\"{ item }\">\r\n          <Btn @click=\"onClickEdit(item.id)\" theme=\"info\">Изменить</Btn>\r\n          <Btn @click=\"onClickRemove(item.id)\" theme=\"danger\">Удалить</Btn>\r\n        </template>\r\n      </Table>\r\n      <router-link :to=\"{ name: 'PaintingEdit' }\">\r\n        <Btn :class=\"$style.create\" theme=\"info\">Создать</Btn>\r\n      </router-link>\r\n    </div>\r\n  </template>\r\n  \r\n  <script>\r\n  import { useStore } from 'vuex';\r\n  import { computed, onMounted } from 'vue';\r\n  import { useRouter } from 'vue-router';\r\n  \r\n  import {selectItemsByHallId, removeItem, fetchItems } from '@/store/paintings/selectors';\r\n  import  {selectItemById } from '@/store/halls/selectors';\r\n  import Table from '@/components/Table/Table';\r\n  import Btn from '@/components/Btn/Btn';\r\n  \r\n  export default {\r\n    name: 'HallPaintingsPage',\r\n    components: {\r\n      Table,\r\n      Btn,\r\n    },\r\n    setup() {\r\n      const store = useStore();\r\n      const router = useRouter();\r\n  \r\n      onMounted(() => {\r\n        fetchItems(store);\r\n      });\r\n  \r\n      const hallId = router.currentRoute.value.query.hallId;\r\n      const items = computed(() => {\r\n            const paintings = selectItemsByHallId(store, hallId);\r\n            return paintings.map(painting => {\r\n            const hall = selectItemById(store, painting.id_hall);\r\n            return { ...painting, id_hall: hall ? hall.name : '' };\r\n            });\r\n        });\r\n  \r\n      const onClickRemove = id => {\r\n        const isConfirmRemove = confirm('Вы действительно хотите удалить запись?')\r\n        if (isConfirmRemove) {\r\n          removeItem(store, id)\r\n        }\r\n      };\r\n  \r\n      const onClickEdit = id => {\r\n        router.push({ name: 'PaintingEdit', params: { id } })\r\n      };\r\n      const getImgUrl = imgPath => {\r\n      return require('@/assets/' + imgPath.slice(7));\r\n      };\r\n  \r\n  \r\n      return {\r\n        items,\r\n        onClickRemove,\r\n        onClickEdit,\r\n        getImgUrl\r\n        \r\n      };\r\n    },\r\n  }\r\n  </script>\r\n  \r\n  <style module lang=\"scss\">\r\n  .root {\r\n    .create {\r\n      margin-top: 16px;\r\n    }\r\n  }\r\n  </style>\r\n  "]}]}